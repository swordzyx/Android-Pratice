<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.constraintlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ConstraintLayout"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.constraintlayout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".GuideLineActivity"></activity>
        <activity android:name=".GroupActivity"></activity>
        <activity android:name=".LayerActivity"></activity>
        <activity android:name=".CircularRevealActivity"></activity>
        <activity android:name=".PlaceHolder"></activity>
        <activity android:name=".ConstraintSetX"></activity>
        <activity android:name=".LinearActivity"></activity>
        <activity android:name=".ConstraintSetStart"></activity>
    </application>

</manifest>