<resources>
    <!-- Base application theme.   <item name="actionBarStyle">@style/ActionBar.Transparent.Start</item> -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    
    <style name="ActionBar.Transparent.Start" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">#ff527da3</item>
        <item name="android:logo">@drawable/transparent</item>
        <item name="android:title">""</item>
    </style>

</resources>