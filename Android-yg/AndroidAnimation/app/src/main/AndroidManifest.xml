<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blog.zero.com.androidanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GestureDetectorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerAnimationActivity" />
        <activity android:name=".GridAnimationActivity">

        </activity>
        <activity android:name=".TransitionActivity" />
        <activity android:name=".ValueAnimatorActivity" />
        <activity android:name=".AnimationActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>